BASIC_AUTH_PASSWORD=password

# Firebase storage keys for package build results
# modules-v2: Original data from package-build-stats <= 7.x
# modules-v3: New data from package-build-stats 8.x+
# When FIREBASE_READ_KEY is 'modules-v3', reads will try v3 first, then fall back to v2
# When FIREBASE_READ_KEY is 'modules-v2', reads will only use v2 (no fallback)
FIREBASE_READ_KEY=modules-v3
FIREBASE_WRITE_KEY=modules-v3

# Firebase storage keys for package exports
FIREBASE_READ_KEY_EXPORTS=exports-v3
FIREBASE_WRITE_KEY_EXPORTS=exports-v3